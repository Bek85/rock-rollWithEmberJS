{{#if hasBlock}}
  {{yield stars}}
{{else}}
  {{#each this.stars as |star|}}
    <button
      type="button"
      class="rating-star"
      {{on "click" (fn this.setRating star.rating)}}
    >
      {{#if star.isFull}}
        <FaIcon @icon="star" />
      {{else}}
        <FaIcon @icon="star" @prefix="far" />
      {{/if}}
    </button>
  {{/each}}
  {{!-- {{log stars}} --}}
{{/if}}